{
  "name": "yorick-server",
  "version": "1.0.0",
  "private": true,
  "packageManager": "yarn@4.4.1",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "yarn run build:types && yarn run build:js",
    "build:types": "tsc",
    "build:js": "webpack",
    "deploy": "copyfiles --up 1 --exclude \"**/dependencies.txt\" \"KoLmafia/**/*\" \"mafia/**/*\"",
    "lint": "eslint src && prettier --check .",
    "lint:fix": "eslint src --fix && prettier --check --write .",
    "watch": "yarn run build:types && yarn run watch:js",
    "watch:js": "webpack --watch --progress"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "core-js": "^3.38.1",
    "kolmafia": "^5.28027.0",
    "libram": "^0.9.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/compat-data": "^7.25.2",
    "@babel/core": "^7.25.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-typescript": "^7.24.7",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@typescript-eslint/parser": "^8.2.0",
    "babel-loader": "^9.1.3",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-libram": "^0.4.11",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4"
  }
}
